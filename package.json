    "mkdirp": "~0.3.0",
    "archy": "~0.0.2",
    "extend": "^1.2.1"
  },
  "devDependencies": {
    "tap": "~0.2.4",
    "seq": "~0.3.5",
    "ronn": "~0.3.8"
  },
  "engines": {
    "node": ">=0.6.0"
  },
