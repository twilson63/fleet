{
    "name" : "fleet",
    "description" : "command a cluster of processes ",
    "version" : "0.1.3",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/fleet.git"
    },
    "main" : "index.js",
    "bin" : {
        "fleet" : "bin/command.sh",
        "fleet-rootdir" : "bin/rootdir.js",
        "fleet-help" : "bin/help.sh",
